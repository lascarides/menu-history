<h1>Listing menu_section_instances</h1>

<table>
  <tr>
    <th>Menu page</th>
    <th>Menu section</th>
    <th>X pos</th>
    <th>Y pos</th>
    <th>Width</th>
    <th>Width</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @menu_section_instances.each do |menu_section_instance| %>
  <tr>
    <td><%= menu_section_instance.menu_page_id %></td>
    <td><%= menu_section_instance.menu_section_id %></td>
    <td><%= menu_section_instance.x_pos %></td>
    <td><%= menu_section_instance.y_pos %></td>
    <td><%= menu_section_instance.width %></td>
    <td><%= menu_section_instance.width %></td>
    <td><%= link_to 'Show', menu_section_instance %></td>
    <td><%= link_to 'Edit', edit_menu_section_instance_path(menu_section_instance) %></td>
    <td><%= link_to 'Destroy', menu_section_instance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

